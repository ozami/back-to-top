{
    "manifest_version": 3,

    "name": "Back to Top",
    "description": "Instantly jump to the top of your active webpage with a single click.",
    "version": "1.1",
    "icons": {
      "24": "icon24.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },

    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
      "default_icon": {
        "24": "icon24.png",
        "32": "icon32.png",
        "48": "icon48.png"
      }
    }
}
